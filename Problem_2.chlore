import "stdlib.chlore"
import "io.chlore"

:main
pushi 0 str0 ;store the result in general purpose register 0

pushi 0
pushi 1

:compute
swap over addl
dup pushl 4000000 pushp end jle
dup pushi 2 swap reml pushp divisible jz
pushp compute jmp

:divisible
dup ldr0 addl str0
pushp compute jmp

:end
ldr0
pushp putl call
hlt
